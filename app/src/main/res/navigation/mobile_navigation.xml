<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="ir.intrack.sdk.demo.ui.MainFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_main" />

    <fragment
        android:id="@+id/nav_events"
        android:name="ir.intrack.sdk.demo.ui.EventsFragment"
        android:label="@string/menu_events"
        tools:layout="@layout/fragment_events" />

    <fragment
        android:id="@+id/nav_user_details"
        android:name="ir.intrack.sdk.demo.ui.UserDetailFragment"
        android:label="@string/menu_user_details"
        tools:layout="@layout/fragment_user_details" />

    <fragment
        android:id="@+id/nav_push_notification"
        android:name="ir.intrack.sdk.demo.ui.PushFragment"
        android:label="@string/menu_push_notification"
        tools:layout="@layout/fragment_push" />

    <fragment
        android:id="@+id/nav_crash"
        android:name="ir.intrack.sdk.demo.ui.CrashFragment"
        android:label="@string/menu_crash"
        tools:layout="@layout/fragment_crash" />
</navigation>